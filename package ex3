package pack1;
class Third
{
void show3()
{
First1 f1=new First1();
//System.out.println("a="+f1.a);
System.out.println("b="+f1.b);
System.out.println("c="+f1.c);
System.out.println("d="+f1.d);
}
public static void main(String[] args)
{
Third t1=new Third();
t1.show3();
}
}

OUTPUT:
b=200
c=300
d=400
