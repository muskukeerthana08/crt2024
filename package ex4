//other package child class
package pack2;
import pack1.First1;
class Fourth extends First1
{
void show4()
{
 //System.out.println("a="+a);
//System.out.println("b="+b);
System.out.println("c="+c);
System.out.println("d="+d);
}
public static void main(String[] args)
{
Fourth f1=new Fourth();
f1.show4();
}
}
